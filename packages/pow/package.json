{
  "name": "@helixnetwork/pow",
  "version": "1.0.0-alpha.2",
  "description": "Utilities for running Proof of Work",
  "main": "./out/src/index.js",
  "typings": "typings/src",
  "publishConfig": {
    "access": "private"
  },
  "scripts": {
    "prepare": "tsc",
    "test": "tsc && nyc ava",
    "test-ci": "nyc ava",
    "format": "prettier",
    "docs":
      "tsc && jsdoc2md --no-cache --plugin dmd-clear -t README_tpl.hbs --files './out/**/*.js' > README.md"
  },
  "contributors": [],
  "ava": {
    "files": "out/test/*.test.js",
    "failFast": false,
    "failWithoutAssertions": false,
    "compileEnhancements": false,
    "verbose": true
  },
  "nyc": {
    "watermarks": {
      "functions": [80, 95],
      "branches": [80, 95],
      "statements": [80, 95]
    },
    "include": ["out/*/src/*.js", "out/*/test/*.js"]
  },
  "keywords": ["helix", "pow"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helixnetwork/helix.api/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/helixnetwork/helix.api.git/tree/dev/packages/pow"
  },
  "dependencies": {
    "@helixnetwork/transaction": "^1.0.0-alpha.2",
    "@helixnetwork/converter": "^1.0.0-alpha.2",
    "@helixnetwork/sha3": "^1.0.0-alpha.2",
    "bcrypto": "^2.0.0"
  }
}
